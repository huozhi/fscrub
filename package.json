{
  "name": "fscrub",
  "version": "0.2.0",
  "description": "universal scrub listener for devices support mouse or touch",
  "main": "dist/fscrub.js",
  "module": "dist/fscrub.esm.js",
  "scripts": {
    "start": "parcel example/index.html -d example/dist --cache-dir example/.cache",
    "docs": "rm -rf exmaple/dist && parcel build example/index.html -d example/dist --public-url /fscrub/ --no-source-maps --no-cache",
    "docs:update": "npm run docs && gh-pages -d ./example/dist",
    "build": "bunchee src/fscrub.js"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huozhi/fscrub.git"
  },
  "keywords": [
    "scrub",
    "touch",
    "mouse",
    "universal-scrub",
    "compatible"
  ],
  "author": "huozhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huozhi/fscrub/issues"
  },
  "homepage": "https://github.com/huozhi/fscrub#readme",
  "devDependencies": {
    "bunchee": "^0.2.1",
    "cssnano": "^4.1.10",
    "gh-pages": "^2.0.1"
  }
}
